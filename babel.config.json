{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "60",
          "ie": "11"
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    [
      "@babel/preset-react"
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ],
    // [
    //   "import",
    //   {
    //     "libraryName": "antd",
    //     "style": "css" // `style: true` 会加载 less 文件
    //   }
    // ],
    [
      "@babel/plugin-syntax-dynamic-import"
    ],
    [
      "@loadable/babel-plugin"
    ]
  ]
}
